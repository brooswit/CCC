local store = {}

function load()
  if fs.exists("disk/config.json") or fs.exists("config.json")
    local file = fs.open("disk/config.json", "r") or fs.open("config.json", "r")
    serializedConfig = file.readAll()
    file.close()
  
    store = textutils.unserialize(serializedConfig)
  end
end

function get(key)
  load()
  return store[key]
end
